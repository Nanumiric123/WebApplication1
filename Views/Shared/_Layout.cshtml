                    <ul class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                        <li class="dropdown-submenu">
                            <a class="dropdown-item dropdown-toggle" href="#">Recieving</a>
                            <ul class="dropdown-menu">
                                <li>@Html.ActionLink("Receiving", "RECEIVING", "Home", new { area = "" }, new { @class = "dropdown-item" })</li>
                                <li>@Html.ActionLink("Receiving Labelling", "RECEIVING_LABELLING", "Home", new { area = "" }, new { @class = "dropdown-item" })</li>
                                <li>@Html.ActionLink("PCB Labelling", "PCB_LABELLING", "Home", new { area = "" }, new { @class = "dropdown-item" })</li>
                            </ul>
                        </li>
                        <li class="dropdown-submenu">
                            <a class="dropdown-item dropdown-toggle" href="#">(Main Store)</a>
                            <ul class="dropdown-menu">
                                <li>@Html.ActionLink("Stock In", "STOCK_IN", "Home", new { area = "" }, new { @class = "dropdown-item" })</li>
                                <li>@Html.ActionLink("Stock Out", "STOCK_OUT", "Home", new { area = "" }, new { @class = "dropdown-item" })</li>
                            </ul>
                        </li>
                        <li>@Html.ActionLink("Kitting", "KITTING", "Home", new { area = "" }, new { @class = "dropdown-item" })</li>
                        <li>@Html.ActionLink("Drop In", "DROP_IN", "Home", new { area = "" }, new { @class = "dropdown-item" })</li>
                        <li>@Html.ActionLink("Supermarket", "SUPERMARKET", "Home", new { area = "" }, new { @class = "dropdown-item" })</li>
                    </ul>
                </li>
                <li class="nav-item active">
                    @Html.ActionLink("Statistics", "Chart", "KANBAN_MASTER", new { area = "" }, new { @class = "nav-link" })
                </li>
            </ul>
        </div>

    </nav>

    <div class="container body-content">

    </div>
    <div class="container-fluid text-body">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
