
@{
    ViewBag.Title = "Generate SMT Store Rack Label";
}

<h2>Generate SMT Store Rack Label</h2>
<br />
<br />
<br />
@using (Html.BeginForm("smtlabelpdf", "Home", FormMethod.Get, new { enctype = "multipart/form-data" }))
{
    <div class="form-group">
        Enter Part Number 1: @Html.TextBox("PN1") 
        <hr />
        Enter Part Number 2: @Html.TextBox("PN2")
        <hr />
        Enter Part Number 3: @Html.TextBox("PN3") 
        <hr />
        Enter Part Number 4: @Html.TextBox("PN4") 
        <hr />
        Enter Part Number 5: @Html.TextBox("PN5") 
        <hr />
        Enter Part Number 6: @Html.TextBox("PN6") 
        <hr />
        Enter Part Number 7: @Html.TextBox("PN7") 
    </div>

    <div>
        <input id="GenerateSMTStoreRackLabel" type="submit" value="submit" />
    </div>
}